      <div class="android-content mdl-layout__content">
        <a name="top"></a>
        <div class="android-be-together-section mdl-typography--text-center">
          <%= video_tag "video.mp4", muted: true, controls: false, :autoplay => true, :loop => true, id: "homepage_video" %>
          <div class="logo-font android-slogan">More than just a number</div>
          <div class="logo-font android-sub-slogan">This is a bad font</div>
